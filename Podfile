# Uncomment the next line to define a global platform for your project
platform :ios, '12.0'

target 'googleSingIn' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for googleSingIn
  pod 'GoogleSignIn'
  pod 'FacebookCore'
  pod 'FacebookLogin'
  pod 'FacebookShare'
  pod 'TwitterKit'
  pod 'LinkedinSwift', '~> 1.7.7'

  target 'googleSingInTests' do
    inherit! :search_paths
    # Pods for testing
  end

end
